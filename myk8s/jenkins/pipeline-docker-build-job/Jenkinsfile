pipeline {
  
   agent {
        node {
            label 'Master'
        }
    }
  
  stages {
    
     stage('print stage') {
      
            steps {
                sh '''
                
                echo "stage 1"
                sleep 20
                
                '''
             
            }
        }
    stage('Docker build') {
      
      steps {
        sh  ''' 
        docker --version
        pwd
        sleep 20
        '''
        }
    }
  }
}
        
        
